{
  "metadata": [
    {
      "src": [
        {
          "src": "./ProjectManager_Server",
          "files": [
            "*.csproj"
          ],
          "includePrivateMembers":true
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net7.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**",
          "changelog/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "outputFormat":"markdown",
    "namespaceLayout":"nested",
    "EnumSortOrder":"declaringOrder",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "ProjectManager",
      "_appTitle": "ProjectManager",
      "_enableSearch": true,
      "_disableContribution": true,
      "_gitContribute": "http://192.168.1.10:3002/Flavien/ProjectManager",
      "_lang":"en",
      "pdf": false
    }
  }
}